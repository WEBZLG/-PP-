<!--pages/3d/book/book.wxml-->
<view>
  <block wx:for="{{bookList}}" wx:key="{{index}}">
    <view class='book' data-id='{{item.id}}' catchtap='viewBook'>
      <image src='../../../image/3dpic.png'></image>
      <view class='content'>{{item.content}}</view>
      <view class='addtime'>{{item.addtime}}</view>
    </view>
  </block>
</view>
